{
	"manifest_version": 3,
	"name": "Booking Automation",
	"version": "1.0",
	"description": "Automates the booking system",
	"icons": {
		"48": "icon/badminton.png"
	},
	"permissions": [
		"activeTab",
		"scripting",
		"tabs"
	],
	"host_permissions": [
		"https://ddasports.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://ddasports.com/*"
			],
			"js": [
				"script/content.js"
			]
		}
	]
}